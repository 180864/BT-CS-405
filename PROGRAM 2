/* program to calculate square of any number using inline functiom*/
#include<iostream>
using namespace std;
class Calc
{
	int a;
	public:
		void getval(int b)
		{
			a=b;
		}
		inline int square(int s)
		{
			 return (s*s);
		}
		void display( int result)
		{
		cout<<"the square of given number is : "<<result<<endl;
};
int main()
{
	int x,result;
	Calc c;
	cout<<"enter value of x : ";
	cin>>x;
	result=c.square(x);
	return 0;
}
